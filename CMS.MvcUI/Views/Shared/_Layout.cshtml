<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewBag.Title</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Unicat project">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="~/Content/FrontEnd/styles/bootstrap4/bootstrap.min.css">
    <link href="~/Content/FrontEnd/plugins/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" type="text/css" href="~/Content/FrontEnd/plugins/OwlCarousel2-2.2.1/owl.carousel.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FrontEnd/plugins/OwlCarousel2-2.2.1/owl.theme.default.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FrontEnd/plugins/OwlCarousel2-2.2.1/animate.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FrontEnd/styles/about.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FrontEnd/styles/about_responsive.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FrontEnd/styles/main_styles.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FrontEnd/styles/responsive.css">


    @RenderSection("HeaderSection", false)

</head>
<body>
    <div class="super_container">

        <!-- Header -->
        @{ Html.RenderAction("HeaderAction", "Home");}

        @RenderBody()

        <!-- Footer -->
        @{ Html.RenderAction("FooterAction", "Home"); }

    </div>


    @RenderSection("ScriptsSection", false)

    <script src="~/Content/FrontEnd/js/jquery-3.2.1.min.js"></script>
    <script src="~/Content/FrontEnd/styles/bootstrap4/popper.js"></script>
    <script src="~/Content/FrontEnd/styles/bootstrap4/bootstrap.min.js"></script>
    <script src="~/Content/FrontEnd/plugins/greensock/TweenMax.min.js"></script>
    <script src="~/Content/FrontEnd/plugins/greensock/TimelineMax.min.js"></script>
    <script src="~/Content/FrontEnd/plugins/scrollmagic/ScrollMagic.min.js"></script>
    <script src="~/Content/FrontEnd/plugins/greensock/animation.gsap.min.js"></script>
    <script src="~/Content/FrontEnd/plugins/greensock/ScrollToPlugin.min.js"></script>
    <script src="~/Content/FrontEnd/plugins/OwlCarousel2-2.2.1/owl.carousel.js"></script>
    <script src="~/Content/FrontEnd/plugins/easing/easing.js"></script>
    <script src="~/Content/FrontEnd/plugins/parallax-js-master/parallax.min.js"></script>

    <script src="~/Content/FrontEnd/plugins/colorbox/jquery.colorbox-min.js"></script>
    <script src="~/Content/FrontEnd/js/about.js"></script>
    <script src="~/Content/FrontEnd/js/blog_single.js"></script>
    <script src="~/Content/FrontEnd/js/contact.js"></script>
    <script src="~/Content/FrontEnd/js/custom.js"></script>


    <script src="~/Content/FrontEnd/js/courses.js"></script>
    <script src="~/Content/FrontEnd/js/course.js"></script>

    <script src="~/Areas/Admin/Content/plugins/jquery-validation/jquery.validate.js"></script>
    <script src="~/Areas/Admin/Content/js/pages/examples/sign-up.js"></script>


</body>
</html>