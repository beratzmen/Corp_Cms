@{
    Layout = null;
    <title>@ViewBag.Title</title>
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>Welcome To | Bootstrap Based Admin Template - Material Design</title>

    @*<link rel="icon" href="~/Areas/Admin/Content/favicon.ico" type="image/x-icon">*@
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,700&subset=latin,cyrillic-ext" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" type="text/css">
    <!-- Bootstrap Core Css -->
    <link href="~/Areas/Admin/Content/plugins/bootstrap/css/bootstrap.css" rel="stylesheet">
    <!-- Waves Effect Css -->
    <link href="~/Areas/Admin/Content/plugins/node-waves/waves.css" rel="stylesheet" />
    <!-- Animation Css -->
    <link href="~/Areas/Admin/Content/plugins/animate-css/animate.css" rel="stylesheet" />
    <!-- Colorpicker Css -->
    <link href="~/Areas/Admin/Content/plugins/bootstrap-colorpicker/css/bootstrap-colorpicker.css" rel="stylesheet" />
    <!-- Dropzone Css -->
    <link href="~/Areas/Admin/Content/plugins/dropzone/dropzone.css" rel="stylesheet">
    <!-- Multi Select Css -->
    <link href="~/Areas/Admin/Content/plugins/multi-select/css/multi-select.css" rel="stylesheet">
    <!-- JQuery DataTable Css -->
    <link href="~/Areas/Admin/Content/plugins/jquery-datatable/skin/bootstrap/css/dataTables.bootstrap.css" rel="stylesheet">

    <!-- Bootstrap Material Datetime Picker Css -->
    <link href="~/Areas/Admin/Content/plugins/bootstrap-material-datetimepicker/css/bootstrap-material-datetimepicker.css" rel="stylesheet" />
    <!-- Wait Me Css -->
    <link href="~/Areas/Admin/Content/plugins/waitme/waitMe.css" rel="stylesheet" />
    <!-- Bootstrap Tagsinput Css -->
    <link href="~/Areas/Admin/Content/plugins/bootstrap-tagsinput/bootstrap-tagsinput.css" rel="stylesheet">
    <!-- Bootstrap Select Css -->
    <link href="~/Areas/Admin/Content/plugins/bootstrap-select/css/bootstrap-select.css" rel="stylesheet" />
    <!-- Morris Chart Css-->
    <link href="~/Areas/Admin/Content/plugins/morrisjs/morris.css" rel="stylesheet" />
    <!-- noUISlider Css -->
    <link href="~/Areas/Admin/Content/plugins/nouislider/nouislider.min.css" rel="stylesheet" />
    <!-- Custom Css -->
    <link href="~/Areas/Admin/Content/css/style.css" rel="stylesheet">
    <!-- AdminBSB Themes. You can choose a theme from css/themes instead of get all themes -->
    <link href="~/Areas/Admin/Content/css/themes/all-themes.css" rel="stylesheet" />
</head>
<body class="theme-red">
    <!-- Page Loader -->
    <div class="page-loader-wrapper">
        <div class="loader">
            <div class="preloader">
                <div class="spinner-layer pl-red">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>
            </div>
            <p>Lütfen Bekleyin ..</p>
        </div>
    </div>
    <!-- #END# Page Loader -->
    <!-- Overlay For Sidebars -->
    <div class="overlay"></div>
    <!-- #END# Overlay For Sidebars -->

    @Html.Partial("SearchBar")
    @Html.Partial("TopBar")
    @Html.Partial("LeftMenu")
    <section class="content">
        <div class="container-fluid">
            @RenderBody()
        </div>
    </section>

    @RenderSection("ScriptsSection", false)

    <!-- Jquery Core Js -->
    <script src="~/Areas/Admin/Content/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap Core Js -->
    <script src="~/Areas/Admin/Content/plugins/bootstrap/js/bootstrap.js"></script>
    <!-- Select Plugin Js -->
    <script src="~/Areas/Admin/Content/plugins/bootstrap-select/js/bootstrap-select.js"></script>
    <!-- Slimscroll Plugin Js -->
    <script src="~/Areas/Admin/Content/plugins/jquery-slimscroll/jquery.slimscroll.js"></script>
    <!-- Waves Effect Plugin Js -->
    <script src="~/Areas/Admin/Content/plugins/node-waves/waves.js"></script>

    <!-- Jquery DataTable Plugin Js -->
    <script src="~/Areas/Admin/Content/plugins/jquery-datatable/jquery.dataTables.js"></script>
    <script src="~/Areas/Admin/Content/plugins/jquery-datatable/skin/bootstrap/js/dataTables.bootstrap.js"></script>
    <script src="~/Areas/Admin/Content/plugins/jquery-datatable/extensions/export/dataTables.buttons.min.js"></script>
    <script src="~/Areas/Admin/Content/plugins/jquery-datatable/extensions/export/buttons.flash.min.js"></script>
    <script src="~/Areas/Admin/Content/plugins/jquery-datatable/extensions/export/jszip.min.js"></script>
    <script src="~/Areas/Admin/Content/plugins/jquery-datatable/extensions/export/pdfmake.min.js"></script>
    <script src="~/Areas/Admin/Content/plugins/jquery-datatable/extensions/export/vfs_fonts.js"></script>
    <script src="~/Areas/Admin/Content/plugins/jquery-datatable/extensions/export/buttons.html5.min.js"></script>
    <script src="~/Areas/Admin/Content/plugins/jquery-datatable/extensions/export/buttons.print.min.js"></script>




    <!-- Autosize Plugin Js -->
    <script src="~/Areas/Admin/Content/plugins/autosize/autosize.js"></script>
    <!-- Moment Plugin Js -->
    <script src="~/Areas/Admin/Content/plugins/momentjs/moment.js"></script>
    <!-- Bootstrap Material Datetime Picker Plugin Js -->
    <script src="~/Areas/Admin/Content/plugins/bootstrap-material-datetimepicker/js/bootstrap-material-datetimepicker.js"></script>
    <!-- Jquery CountTo Plugin Js -->
    <script src="~/Areas/Admin/Content/plugins/jquery-countto/jquery.countTo.js"></script>
    <!-- Morris Plugin Js -->
    <script src="~/Areas/Admin/Content/plugins/raphael/raphael.min.js"></script>
    <script src="~/Areas/Admin/Content/plugins/morrisjs/morris.js"></script>
    <!-- ChartJs -->
    <script src="~/Areas/Admin/Content/plugins/chartjs/Chart.bundle.js"></script>

    <!-- Custom Js -->
    <script src="~/Areas/Admin/Content/js/admin.js"></script>
    <script src="~/Areas/Admin/Content/js/pages/forms/basic-form-elements.js"></script>
    <script src="~/Areas/Admin/Content/js/pages/tables/jquery-datatable.js"></script>
    <script src="~/Areas/Admin/Content/js/pages/index.js"></script>
    <!-- Demo Js -->
    <script src="~/Areas/Admin/Content/js/demo.js"></script>

    <script src="~/Areas/Admin/Content/plugins/bootstrap-notify/bootstrap-notify.js"></script>
    <script src="~/Areas/Admin/Content/js/pages/ui/modals.js"></script>


</body>
</html>
